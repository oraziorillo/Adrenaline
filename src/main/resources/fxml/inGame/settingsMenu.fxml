<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="client.view.gui.javafx_controllers.dialogs.SettingsMenu" fx:id="mainPane"
          prefHeight="400.0" prefWidth="600.0" stylesheets="/css/style.css">

    <Button text="Resume" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.halignment="CENTER" onMouseClicked="#resumeClicked"/>
    <Button text="Game manual" GridPane.rowIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.halignment="CENTER" onMouseClicked="#showGameManual"/>
    <Button text="Weapon manual" GridPane.rowIndex="2" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" GridPane.halignment="CENTER" GridPane.valignment="CENTER" onMouseClicked="#showWeaponManual"/>
    <Button text="Disconnect" GridPane.rowIndex="3" GridPane.hgrow="ALWAYS" GridPane.halignment="CENTER" GridPane.vgrow="ALWAYS" GridPane.valignment="CENTER" onMouseClicked="#disconnect"/>


</GridPane>

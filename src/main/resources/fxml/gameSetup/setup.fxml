<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="client.view.gui.javafx_controllers.game_setup.SetupController"
          fx:id="mainPane" stylesheets="/css/setup.css, /css/style.css"
          prefHeight="400.0" prefWidth="600.0" >

    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" percentWidth="20"/>
        <ColumnConstraints hgrow="ALWAYS" percentWidth="60"/>
        <ColumnConstraints hgrow="ALWAYS" percentWidth="20"/>
    </columnConstraints>

    <Region GridPane.columnSpan="3"/>
    <HBox fx:id="skullTrack" alignment="CENTER" GridPane.rowIndex="1" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.fillWidth="false"/>
    <Region GridPane.rowIndex="2" GridPane.columnSpan="2" GridPane.vgrow="ALWAYS"/>
    <TilePane fx:id="maps" hgap="10" vgap="10" GridPane.rowIndex="3" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.fillWidth="false"/>
    <Region GridPane.rowIndex="4" GridPane.columnSpan="2" GridPane.vgrow="ALWAYS"/>
    <HBox fx:id="pcs" GridPane.rowIndex="5" GridPane.columnIndex="1" GridPane.halignment="CENTER"/>
    <fx:include fx:id="chat" source="/fxml/inGame/chat.fxml" GridPane.rowSpan="6" GridPane.columnIndex="2" GridPane.valignment="BOTTOM"/>


</GridPane>
